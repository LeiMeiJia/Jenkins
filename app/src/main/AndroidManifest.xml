<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aerozhonghuan.jenkins">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mvp.TestActivity" />
        <activity android:name=".rxjava.RxJavaActivity" />

        <activity android:name=".Test1Activity" />
        <activity android:name=".Test2Activity" />
        <activity android:name=".Test3Activity" />
        <activity android:name=".receiver.LocalBroadcastActivity" />
        <activity android:name=".service.TestServiceActivity" />
        <activity android:name=".service.HanderThreadActivity" />

        <service android:name=".service.MyIntentService"/>
        <service android:name=".service.MyService"/>
    </application>

</manifest>